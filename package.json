{
  "name": "create-psrworld",
  "version": "1.0.0",
  "description": "A TypeScript-powered project scaffolding tool with dual CommonJS/ESM build support",
  "keywords": ["create", "psrworld",
    "typescript",
    "cli",
    "scaffolding",
    "templates",
    "framework",
    "dual-package"
  ],
  "author": "psrworld <support@psrworld.in>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/psrworld/psrworld.git"
  },
  "homepage": "https://github.com/psrworld/psrworld#readme",
  "bugs": {
    "url": "https://github.com/psrworld/psrworld/issues"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "create-psrworld": "./bin/create-psrworld.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:cjs && npm run build:esm && npm run build:bin",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "build:cjs": "rollup -c rollup.config.cjs.js",
    "build:esm": "rollup -c rollup.config.esm.js",
    "build:bin": "rollup -c rollup.config.bin.js",
    "clean": "rimraf dist",
    "dev": "rollup -c rollup.config.esm.js --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/cross-spawn": "^6.0.6",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "cross-spawn": "^7.0.3",
    "mri": "^1.2.0",
    "picocolors": "^1.0.0"
  }
}
